   <xs:annotation>
      <xs:appinfo>
         <standard>
            <nameLang>XEvidenceSurvey</nameLang>
            <nameKurz>XEvidenceSurvey</nameKurz>
            <nameTechnisch>xevidencesurvey</nameTechnisch>
