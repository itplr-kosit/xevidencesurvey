         <thead>
            <row>
               <entry namest="indent0" nameend="page" align="center">Kindelemente von <emphasis role="codebf">SDGDistributionType</emphasis>
               </entry>
            </row>
            <row>
               <entry namest="indent0" nameend="child">Kindelement</entry>
